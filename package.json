{
  "name": "pet-adoption-angular",
  "version": "1.0.0",
  "author": "jcprice12",
  "description": "front-end to interact with https://github.com/jcprice12/pet-adoption",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "main": "server.js",
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "start": "node server.js",
    "start:dev": "npm run ng:build:dev && npm run start",
    "ng:start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "ng:build:dev": "set NODE_OPTIONS=--openssl-legacy-provider && ng build",
    "ng:test": "ng test",
    "ng:e2e": "set NODE_OPTIONS=--openssl-legacy-provider && ng e2e",
    "test:jest": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:open:con": "concurrently \"ng serve\" \"cypress open\"",
    "cypress:run:con": "concurrently \"ng serve\" \"cypress run\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.7",
    "@angular/cdk": "^18.2.7",
    "@angular/cli": "18.2.7",
    "@angular/common": "18.2.7",
    "@angular/compiler": "18.2.7",
    "@angular/compiler-cli": "18.2.7",
    "@angular/core": "18.2.7",
    "@angular/forms": "18.2.7",
    "@angular/material": "^18.2.7",
    "@angular/platform-browser": "18.2.7",
    "@angular/platform-browser-dynamic": "18.2.7",
    "@angular/router": "18.2.7",
    "@material/layout-grid": "^14.0.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.1",
    "rxjs": "7.4.0",
    "tslib": "^1.11.1",
    "typescript": "5.4.5",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-builders/jest": "^18.0.0",
    "@angular/build": "^18.2.7",
    "@angular/language-service": "18.2.7",
    "@cypress/schematic": "^1.5.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^16.4.10",
    "concurrently": "^6.2.0",
    "cypress": "^8.2.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.0",
    "prettier": "1.19.1",
    "ts-node": "^10.1.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ]
}