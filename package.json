{
  "name": "pet-adoption-angular",
  "version": "1.0.0",
  "author": "jcprice12",
  "description": "front-end to interact with https://github.com/jcprice12/pet-adoption",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "main": "server.js",
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "start": "node server.js",
    "start:dev": "npm run ng:build:dev && node server.js",
    "ng:start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "ng:build:dev": "set NODE_OPTIONS=--openssl-legacy-provider && ng build",
    "ng:test": "ng test",
    "ng:lint": "ng lint",
    "ng:e2e": "set NODE_OPTIONS=--openssl-legacy-provider && ng e2e",
    "test:jest": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:open:con": "concurrently \"ng serve\" \"cypress open\"",
    "cypress:run:con": "concurrently \"ng serve\" \"cypress run\""
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "13.3.11",
    "@angular/animations": "13.3.11",
    "@angular/cdk": "13.3.9",
    "@angular/cli": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/compiler-cli": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@material/layout-grid": "^13.0.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.1",
    "rxjs": "7.4.0",
    "tslib": "^1.11.1",
    "tslint": "^6.1.3",
    "typescript": "4.4.3",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.4",
    "@angular/language-service": "13.3.11",
    "@cypress/schematic": "^1.5.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.0",
    "cypress": "^8.2.0",
    "jest": "^27.0.6",
    "jest-preset-angular": "11.1.2",
    "prettier": "1.19.1",
    "ts-node": "^10.1.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ]
}
